#include<iostream>
using namespace std;

namespace first{
 int add(int a, int b){
   return (a+b);
  }
}
namespace second{
 float add(float a, float b){
   return(a+b);
  }
}  
int main(){
cout<<first::add(5,12);
cout<<"\n";

cout<<second::add(4.0,8.0);
cout<<"\n";

cout<<first::add(4,8.2);
cout<<"\n";

cout<<second::add(4.2,8);
cout<<"\n";
  return 0;
}
